<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beingyi.apkfairy">


    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        
        <activity
            android:name="com.beingyi.apkfairy.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name"/>


        <activity
            android:name="com.beingyi.apkfairy.activity.setting"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name"/>


        <activity
            android:name="com.beingyi.apkfairy.activity.backuped"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name"/>
        
        <activity
            android:name="com.beingyi.apkfairy.activity.launch"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name"/>
        
        
    </application>

</manifest>
